{
    "manifest_version": 3,
    "name": "DesignDupe",
    "version": "1.0",
    "description": "Extracts colors and fonts from the current webpage",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }